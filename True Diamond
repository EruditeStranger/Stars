#include <iostream>

using namespace std;

int main()
{
int i,j;
for(i=1;i<=5;i++)
{
    for(j=5;j>=i;j--)
    {
        cout<<"*";
    }
    for(j=6;j<=(i+5);j++)
    {
        cout<<" ";
    }
    for(j=7;j<=(i+5);j++)
    {
        cout<<" ";
    }
    for(j=5;j>=i;j--)
    {
        cout<<"*";
    }
    cout<<"\n";
}
for(i=6;i<10;i++)
{
    for(j=5;j<=i;j++)
    {
        cout<<"*";
    }
    for(j=9;j>=i;j--)
    {
        cout<<" ";
    }
    for(j=8;j>=i;j--)
    {
        cout<<" ";
    }
    for(j=5;j<=i;j++)
    {
        cout<<"*";
    }
    cout<<"\n";
}
return 0;
}
