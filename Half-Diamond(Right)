#include <iostream>

using namespace std;

int inverted();
int main()
{
    inverted();
    return 0;
}
int inverted()
{
    int i,j;
    for(i=0;i<=5;i++)
    {
        for(j=1;j<=i;j++)
        {
            cout<<"5";
        }
        for(j=5;j>=i;j--)
        {
            cout<<"*";
        }
        cout<<"\n";
    }
    for(i=10;i>=6;i--)
    {
        for(j=2;j<=(i-5);j++)
        {
            cout<<"5";
        }
        for(j=11;j>=i;j--)
        {
            cout<<"*";
        }
        cout<<"\n";
    }
    return 0;
}
